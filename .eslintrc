{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "space-before-function-paren": [2, "never"],
    "no-use-before-define": [2, { "functions": false }],
    "global-require": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "import/no-extraneous-dependencies": 0
  }
}
