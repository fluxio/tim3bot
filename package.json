{
  "name": "tim3bot",
  "version": "0.0.1",
  "description": "The manager you didn't know you want.",
  "main": "index.js",
  "scripts": {
    "build": "webpack -d",
    "build:min": "webpack -p",
    "clean": "rimraf public",
    "lint": "eslint src test",
    "prepublish": "npm run clean && npm run build:min",
    "prestart": "knex migrate:latest",
    "start": "npm run start:server",
    "start:server": "knex migrate:latest && node ./bin/www",
    "start:server:watch": "nodemon -w src/server -x 'npm run start:server'",
    "start:proxy": "node ./bin/proxy",
    "start:static": "node ./bin/static",
    "start:watch": "cross-env NODE_ENV=development HOT=true concurrently 'npm run start:server:watch' 'npm run start:static' 'npm run start:proxy'",
    "test": "cross-env BABEL_ENV=commonjs mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "connect-redis": "^3.1.0",
    "cookie-parser": "^1.4.3",
    "errorhandler": "^1.4.3",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "knex": "^0.11.10",
    "morgan": "^1.7.0",
    "node-fetch": "^1.6.0",
    "passport": "^0.3.2",
    "passport-slack": "0.0.6",
    "pg": "^6.1.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "redis": "^2.6.2",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "serve-favicon": "^2.3.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "concurrently": "^2.2.0",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.1",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "http-proxy": "^1.14.0",
    "mocha": "^3.0.2",
    "node-sass": "^3.8.0",
    "nodemon": "^1.10.0",
    "redbox-react": "^1.3.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-logger": "^2.6.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
